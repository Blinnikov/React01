{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
   "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "parser": "babel-eslint",
  "plugins": ["react"],
  "rules": {
    "no-console": 0,
    "no-unused-vars": 1
  }
}
